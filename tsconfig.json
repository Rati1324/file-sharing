 {
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],    
    "target": "ESNext",    
    "types": ["vite/client"],
    "isolatedModules": true,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": ["src"],
  "exclude": [
    "node_modules"
  ]
}